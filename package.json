{
  "name": "irontimer",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "description": "A modern and customizable Rubik's Cube timer with inspiration from Monkeytype.",
  "author": "Evan <evanrayweigel@outlook.com>",
  "license": "AGPL-3.0",
  "engines": {
    "node": "16.13.2"
  },
  "scripts": {
    "dev": "yarn workspaces foreach -pvi run dev",
    "build": "yarn workspaces foreach -v run build",
    "pretty": "yarn workspaces foreach -v run pretty",
    "pretty-fix": "yarn workspaces foreach -v run pretty-fix",
    "lint": "yarn workspaces foreach -v run lint",
    "lint-fix": "yarn workspaces foreach -v run lint-fix",
    "start": "yarn workspace @irontimer/backend start"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/node": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "packageManager": "yarn@3.2.1"
}
