  "author": "Evan <evanrayweigel@outlook.com>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@irontimer/utils": "0.0.0",
    "bcrypt": "5.0.1",
    "bullmq": "1.86.3",
    "chalk": "4.1.2",
    "cors": "2.8.5",
    "cron": "2.0.0",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "express-rate-limit": "6.4.0",
    "firebase-admin": "11.0.0",
    "helmet": "5.1.0",
    "ioredis": "5.1.0",
    "joi": "17.6.0",
    "lodash": "4.17.21",
    "mongoose": "6.4.1",
    "node-fetch": "3.2.8",
    "prom-client": "14.0.1",
    "ua-parser-js": "1.0.2",
    "uuid": "8.3.2",
    "winston": "3.8.0"
