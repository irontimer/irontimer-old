  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/cors": "2.8.12",
    "@types/cron": "2.0.0",
    "@types/express-serve-static-core": "4.17.29",
    "@types/lodash": "4.14.182",
    "@types/node-fetch": "2.6.1",
    "@types/ua-parser-js": "0.7.36",
    "@types/uuid": "8.3.4",
    "nodemon": "2.0.18",
    "ts-node": "10.8.1"
  }
