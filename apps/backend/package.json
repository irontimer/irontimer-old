{
  "name": "@irontimer/backend",
  "version": "0.0.0",
  "private": true,
  "description": "Backend for IronTimer",
  "scripts": {
    "dev": "nodemon",
    "start": "yarn build && node dist",
    "build": "yarn tsc",
    "pretty": "prettier \"src/**/*.ts\"",
    "pretty-fix": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint-fix": "eslint --fix \"src/**/*.ts\"",
    "tsc": "yarn dlx --package typescript tsc"
