{
  "name": "@irontimer/frontend",
  "version": "0.0.0",
  "private": true,
  "description": "Frontend for IronTimer",
  "author": "Evan <evanrayweigel@outlook.com>",
  "license": "AGPL-3.0",
  "scripts": {
    "dev": "vite",
    "dev-tauri": "tauri dev",
    "build": "yarn tsc --noEmit && vite build",
    "build-tauri": "tauri build",
    "serve": "vite preview",
    "deploy": "yarn build && firebase deploy --only hosting",
    "pretty": "prettier \"src/**/*.{ts,tsx,scss}\"",
    "pretty-fix": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint-fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "madge": "madge --extensions tsx,ts \"src/ts\" --image madge.png",
    "tsc": "yarn dlx --package typescript tsc"
  },
  "dependencies": {
    "@fortawesome/fontawesome-common-types": "6.1.1",
    "@irontimer/utils": "0.0.0",
    "axios": "0.27.2",
    "firebase": "9.8.4",
    "sass": "1.53.0",
    "solid-app-router": "0.4.1",
    "solid-js": "1.4.4",
    "stackmat": "1.1.0"
  },
  "devDependencies": {
    "@firebase/app-compat": "0.1.28",
    "@tauri-apps/cli": "1.0.0",
    "@types/babel__core": "7.1.19",
    "rollup": "2.76.0",
    "rollup-plugin-visualizer": "5.6.0",
    "vite": "2.9.13",
    "vite-plugin-solid": "2.2.6"
  }
}
