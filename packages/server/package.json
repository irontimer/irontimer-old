{
  "name": "server",
  "scripts": {
    "dev": "npx ts-node-dev \"./src\" --enable-source-maps --project \"tsconfig.json\" --ext \"ts,json\"",
    "start": "yarn workspace server build && node dist",
    "build": "npx tsc",
    "pretty": "prettier \"src/**/*.{ts,json}\"",
    "pretty-fix": "prettier --write \"src/**/*.{ts,json}\"",
    "lint": "eslint \"src/**/*.{ts,json}\"",
    "lint-fix": "eslint --fix \"src/**/*.{ts,json}\""
  },
  "dependencies": {
    "bcrypt": "5.0.1",
    "bullmq": "1.86.3",
    "chalk": "4.1.2",
    "cors": "2.8.5",
    "cron": "2.1.0",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "express-rate-limit": "6.4.0",
    "firebase-admin": "11.0.0",
    "helmet": "5.1.0",
    "ioredis": "5.1.0",
    "joi": "17.6.0",
    "lodash": "4.17.21",
    "mongoose": "6.4.1",
    "node-fetch": "2.6.7",
    "prom-client": "14.0.1",
    "ua-parser-js": "1.0.2",
    "utils": "workspace:^",
    "uuid": "8.3.2",
    "winston": "3.8.1"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/cors": "2.8.12",
    "@types/cron": "2.0.0",
    "@types/express-serve-static-core": "4.17.29",
    "@types/lodash": "4.14.182",
    "@types/node-fetch": "2.6.1",
    "@types/ua-parser-js": "0.7.36",
    "@types/uuid": "8.3.4"
  },
  "packageManager": "yarn@3.2.2"
}
